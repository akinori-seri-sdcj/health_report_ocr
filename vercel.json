{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": ".next",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "ignoreCommand": "echo \"Skipping api/ and backend/ for Next.js API Routes deployment\""
}

