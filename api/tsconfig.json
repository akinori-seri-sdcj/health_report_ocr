{
  "extends": "../backend/tsconfig.json",
  "compilerOptions": {
    "module": "esnext",
    "target": "es2020",
    "lib": ["es2020"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": false,
    "outDir": ".vercel/output"
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}

